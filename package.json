{
  "name": "openapi-to-rescript-generator",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev-start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "8"
      }
    }
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "@apidevtools/swagger-parser": "^10.1.0",
    "handlebars": "^4.7.7",
    "handlebars-helpers": "^0.10.0",
    "minimist": "^1.2.8",
    "openapi3-ts": "^4.1.2",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/handlebars-helpers": "^0.5.3",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "author": "CoopCoding",
  "module": "dist/openapi-to-rescript-generator.esm.js",
  "size-limit": [
    {
      "path": "dist/openapi-to-rescript-generator.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/openapi-to-rescript-generator.esm.js",
      "limit": "10 KB"
    }
  ]
}
