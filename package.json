{
  "name": "openapi-to-rescript-generator",
  "version": "0.1.2",
  "author": "CoopCoding <coop.coding@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=15"
  },
  "main": "src/index.bundle.mjs",
  "bin": {
    "openapi-to-rescript": "./src/index.bundle.mjs"
  },
  "type": "module",
  "keywords": [
    "rescript",
    "openapi"
  ],
  "scripts": {
    "dev-start": "rescript build -w",
    "start": "node ./src/index.bs.mjs",
    "//": "need to delete first, otherwise rescript ignores if not changed and make-file-executable ads a second shebang",
    "build": "trash src/index.bs.mjs && rescript build -with-deps",
    "bundle": "esbuild src/index.bs.mjs --bundle --platform=node --tree-shaking=true --format=esm --outfile=src/index.bundle.mjs",
    "build-publish": "npm run build && npm run bundle && npm run make-file-executable && npm publish",
    "make-file-executable": "./make-file-exec.sh",
    "publish-dry-run": "npm run build && npm run bundle && npm run make-file-executable && npm pack --dry-run"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "@apidevtools/swagger-parser": "^10.1.0",
    "handlebars": "^4.7.7",
    "minimist": "^1.2.8",
    "rescript": "^10.1.4",
    "rescript-json-schema": "^3.0.0",
    "rescript-nodejs": "^14.4.0",
    "rescript-struct": "^4.1.1"
  },
  "devDependencies": {
    "esbuild": "^0.17.19",
    "eslint": "^8.41.0",
    "trash-cli": "^5.0.0"
  },
  "peerDependencies": {
    "node-fetch": "^3.3.1"
  }
}
