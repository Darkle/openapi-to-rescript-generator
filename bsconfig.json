{
  "name": "openapi-to-rescript-generator",
  "version": "0.1.0",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.mjs",
  "bs-dependencies": ["rescript-nodejs", "rescript-json-schema", "rescript-struct"],
  "bsc-flags": ["-open RescriptStruct"]
}
